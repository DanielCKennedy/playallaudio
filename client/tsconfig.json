{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "typeRoots": [
      "./@types",
      "./node_modules/@types"
    ]
  },
  "include": [
    "src", "../TestComponent1.tsx", "../PlayallPlayer.test.tsx", "../PlayallPlayer copy.tsx"
  ],
  "exclude": [
    "node_modules",
    "typings"
  ]
}
